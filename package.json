{
  "name": "thirdfort-nikolai-momot",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "check": "npm run format && npm run lint",
    "dev": "nodemon index.js",
    "format": "prettier --write \"**/*.+(js|yml|yaml|json)\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node index.js",
    "test": "NODE_ENV=test jest'",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:connect": "psql -h localhost -p 5432 -U nikolai -d thirdfort -W",
    "migrate:up": "yarn knex migrate:latest",
    "migrate:down": "yarn knex migrate:rollback",
    "migrate:new": "yarn knex migrate:make $1"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jest": "22.5.1",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "2.3.0",
    "nodemon": "1.19.0",
    "prettier": "1.17.1"
  },
  "dependencies": {
    "@koa/cors": "2.2.3",
    "axios": "^0.27.2",
    "dotenv": "8.0.0",
    "joi": "14.3.1",
    "knex": "^2.1.0",
    "koa": "2.7.0",
    "koa-bodyparser": "4.2.1",
    "koa-compress": "3.0.0",
    "koa-helmet": "4.1.0",
    "koa-logger": "3.2.0",
    "koa-router": "7.4.0",
    "pg": "^8.7.3",
    "uuid": "3.3.2"
  }
}
